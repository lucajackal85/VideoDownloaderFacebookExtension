{
    "name": "jackal/video-download-ext-facebook",
    "description": "\"Download Facebook Video\"",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "jackal/video-downloader": "^0.4",
        "guzzlehttp/guzzle": "^6.5",
        "symfony/dom-crawler": "^3.4",
        "symfony/css-selector": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "brainmaestro/composer-git-hooks": "^2.8",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Jackal\\Downloader\\Ext\\Facebook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jackal\\Downloader\\Ext\\Facebook\\Tests\\": "tests/"
        }
    },
    "extra": {
        "hooks": {
            "pre-commit": [
                "echo Committing as $(git config user.name) [$(git config user.email)]",
                "vendor/bin/php-cs-fixer fix . --config=.php-cs",
                "sleep 1"
            ],
            "pre-push": [
                "vendor/bin/phpunit tests/"
            ]
        }
    },
    "authors": [
        {
            "name": "Luca",
            "email": "luca.giacalone@edimotive.com"
        }
    ]
}
